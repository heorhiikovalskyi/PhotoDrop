{
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "installdrizzleKit": "npm i -D drizzle-kit",
    "generate": "drizzle-kit generate:pg",
    "start": "npm run build && npm run server",
    "eslint": "eslint ./src",
    "eslint:fix": "npx eslint --fix src/**",
    "build": "esbuild src/server.ts --platform=node --target=node16.17.0 --format=esm --bundle --minify --main-fields=main --outfile=dist/server.js && npm run insertInDist",
    "insertInDist": "copy distPackage.json dist && copy package-lock.json dist && npm run renameFile",
    "renameFile": "node -e \"require('fs').rename('dist/distPackage.json', 'dist/package.json', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "dev": "nodemon src/server.ts",
    "server": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.456.0",
    "@aws-sdk/s3-presigned-post": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.461.0",
    "@types/aws-lambda": "^8.10.129",
    "aws-sdk": "^2.1502.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.6",
    "esbuild": "^0.19.5",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "drizzle-kit": "^0.19.13"
  }
}
